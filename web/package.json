    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "core-js": "^2.6.11",
    "rest-api-url-builder": "^1.2.2",
    "rxjs": "^6.5.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "^8.3.25",
