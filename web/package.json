{
  "name": "scaleapp",
  "version": "0.0.0",
  "scripts": {
    "start-local": "ng serve --proxy-config src/environments/proxy.conf.local.json -c local",
    "start-dev": "ng serve --proxy-config src/environments/proxy.conf.dev.json -c dev",
    "start-prod": "ng serve --proxy-config src/environments/proxy.conf.prod.json -c prod",
    "ng": "ng",
    "build-local": "ng build --configuration=local",
    "build-dev": "ng build --configuration = dev",
    "build-prod": "ng build --configuration = prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
